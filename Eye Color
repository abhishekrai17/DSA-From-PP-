#include <iostream>
using namespace std;
int main() {
   char arr[2];
  
   for(int i=0;i<2;i++){
       cin>>arr[i];
   }
   for(int i=0;i<1;i++){
       if(arr[i]==arr[i+1]){
           cout <<arr[i];
       }
        else if(arr[i]!=arr[i+1])
        {
         if((arr[i]=='R') || (arr[i+1]=='R')){
           cout <<'R';
       }else if((arr[i]=='B') && (arr[i+1]!='R')){
           cout<<'B';
       
       }else if((arr[i+1]=='B') && (arr[i]!='R')){
           cout<<'B';
       }
   }}
   
   
   
    return 0;
}

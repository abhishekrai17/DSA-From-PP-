#include <iostream>
using namespace std;
int main() {
    int testcases;
    cin>>testcases;
    int j=1;
    while(j<=testcases){
        int n;
        cin>>n;
        int i=1;
        int count=0;
        while(n>0){
            n-=i;
            i+=9;
            count++;
        }
        cout<<"Case #"<<j<<": " << count <<endl;
        j++;
    }
    
    return 0;
}

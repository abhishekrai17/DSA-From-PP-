#include <iostream>
using namespace std;
int main() {
  int testcases;
  
  cin>>testcases;
  while(testcases--){
      int n,k;
       int count=0;
      cin>>n>>k;
      int arr[n];
      for(int i=0;i<n;i++){
          cin>>arr[i];
           int sum=0;
          sum=arr[i]+k;
          if(sum %7==0){
              count++;
          }
      }
      
      cout << count<<endl ;
  }
  
    return 0;
}

#include <iostream>
using namespace std;
int main() {
    int testCases;
    cin>>testCases;
    while(testCases--){
        int a,b;
        cin>>a>>b;
        if(a>b){
            cout<<">"<<endl;
        }
        else if(a<b){
            cout<<"<"<<endl;
        }
        if(a==b){
            cout<<"="<<endl;
        }
    }
    return 0;
}
